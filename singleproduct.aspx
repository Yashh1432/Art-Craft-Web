<asp:Content ID="Content2" ContentPlaceHolderID="ContentPlaceHolder1" runat="Server">

    <div class="site-wrap">
        <div class="bg-light py-3">
            <div class="container">
                <div class="row">
                    <div class="col-md-12 mb-0">
                        <asp:HyperLink ID="hlHome" runat="server" NavigateUrl="Default.aspx" Text="Home"></asp:HyperLink>
                        <span class="mx-2 mb-0">/</span>
                        <strong class="text-black"></strong>
                    </div>
                </div>
            </div>
        </div>

        <div class="site-section">
            <div class="container">
                <div class="row">
                    <asp:ScriptManager runat="server" />
                    <asp:UpdatePanel runat="server" ID="updatepanel1">
                        <ContentTemplate>
                            <div class="col-md-6">
                                <asp:Image ID="imgProduct" runat="server" CssClass="img-fluid card-img-top" AlternateText="Image" height="300" BorderWidth="1" BorderColor="Black" BorderStyle="Solid" ></asp:Image>
                            </div>
                            <div class="col-md-6">
                                <h2 style="color: black; font-family: 'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', 'Arial', 'sans-serif';">
                                    <asp:Label runat="server" ID="txtpname"></asp:Label>
                                    [<asp:Label runat="server" ID="txtbrand"></asp:Label>]</h2>
                                <hr />
                                <h3>
                                    <asp:Label runat="server" ID="txtdescription" Style="font-size: 2.5rem;"></asp:Label>
                                </h3>
                                <h3>&#8377;
                            <asp:Label runat="server" ID="txtprice" Style="font-size: 2.3rem;"></asp:Label>.00</h3>

                                <div class="container mt-5">
                                    <div class="mb-5 h2">
                                        <div class="input-group mb-3" style="max-width: 180px;">
                                            <div class="input-group-prepend">
                                                <button id="btnMinus" class="btn btn-outline-primary p-3" onclick="decrement()" type="button" runat="server" style="font-size: 1.5rem;">&minus;</button>
                                            </div>
                                            <asp:TextBox ID="txtQuantity" runat="server" CssClass="form-control text-center p-4" Text="1" Style="font-size: 1.5rem;"></asp:TextBox>
                                            <div class="input-group-append">
                                                <button id="btnPlus" class="btn btn-outline-primary p-3" onclick="increment()" type="button" runat="server" style="font-size: 1.5rem;">&plus;</button>
                                            </div>
                                        </div>
                                    </div>




                                </div>
                                <asp:Button ID="btnAddToCart" runat="server" Text="Add To Cart" OnClick="btnAddToCart_Click" CssClass="buy-now btn btn-sm btn-primary"></asp:Button>
                                &nbsp;<asp:Button ID="wishlist" runat="server" Text="Wishlist" OnClick="wishlist_Click" CssClass="buy-now btn btn-sm btn-primary p-auto"></asp:Button>

                            </div>
                        </ContentTemplate>
                    </asp:UpdatePanel>

                </div>
            </div>
        </div>
        
    <script>
        // Function to show toastr message
        function showToast(message) {
            toastr.success(message);

        }
        function showToastdanget(message) {
            toastr.error(message);

        }
    </script>
        <script>
            function increment() {
                var textBox = document.getElementById('<%= txtQuantity.ClientID %>');
                var currentValue = parseInt(textBox.value);
                textBox.value = currentValue + 1;
            }

            function decrement() {
                var textBox = document.getElementById('<%= txtQuantity.ClientID %>');
                var currentValue = parseInt(textBox.value);
                if (currentValue > 1) {
                    textBox.value = currentValue - 1;
                }
            }
        </script>


    </div>

